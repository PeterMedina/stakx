---
permalink: /calendar/%basename/
dataset: calendar
title: %basename
---

{% extends "_layouts/base.html.twig" %}

{% block content %}
    <p>Events for the month of {{ this.title | title }}</p>

    {% for date in this %}
        <article>
            <p><strong>{{ date.localName }}</strong></p>
            <p>{{ date.date | date('F d, Y') }}</p>
        </article>
    {% endfor %}
{% endblock %}
